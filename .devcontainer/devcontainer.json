{
  "name": "Python 3.10 Codespace",
  "build": {
    "dockerfile": "Dockerfile",
    "args": {
      "VARIANT": "3.11"
    }
  },
  "settings": {
    "python.defaultInterpreterPath": "/usr/local/python3.11/bin/python"
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "extensions": [
    "ms-python.python"
  ]
}
